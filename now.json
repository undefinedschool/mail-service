{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node-server" }],
  "routes": [
    {
      "src": "/health",
      "dest": "index.js",
      "methods": ["GET"]
    },
    {
      "src": "/payments",
      "dest": "index.js",
      "methods": ["OPTIONS", "POST", "HEAD"]
    }
  ],
  "env": {
    "EMAIL_USER": "@email_user",
    "EMAIL_PASS": "@email_pass"
  }
}
