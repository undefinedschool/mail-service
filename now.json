{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node-server" }],
  "routes": [
    {
      "src": "/health",
      "dest": "index.js",
      "methods": ["GET"]
    },
    {
      "src": "/payments",
      "dest": "index.js",
      "methods": ["OPTIONS", "POST", "HEAD"]
    }
  ],
  "env": {
    "NODE_ENV": "@node_env",
    "EMAIL_USER": "@email_user",
    "CLIENT_ID": "@client_id",
    "CLIENT_SECRET": "@client_secret",
    "REFRESH_TOKEN": "@refresh_token"
  }
}
